{
  "manifest_version": 2,
  "name": "Dateifier",
  "version": "1.0",
  "description": "Navigate to archived versions of the current page on web.archive.org.",

  "permissions": [
    "activeTab",
    "https://web.archive.org/*"
  ],

  "browser_action": {
    "default_icon": {
	"48": "icon-light48.png"
    },
    "default_title": "Dateifier",
    "default_popup": "popup.html",
    "theme_icons": [
	    {
		    "light": "icon-dark48.png",
		    "dark": "icon-light48.png",
		    "size": 48
	    }
    ]
  }
}
